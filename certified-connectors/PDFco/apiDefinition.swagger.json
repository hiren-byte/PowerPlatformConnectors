        "summary": "PDF Filler",
        "description": "Add text, images, signatures to pdf. Fill PDF form fields and create new pdf from templates",
        "operationId": "PDF_Filler",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "async": {
                  "type": "boolean",
                  "description": "",
                  "title": "async",
                  "enum": [true, false],
                  "default": false,
                  "x-ms-visibility": "advanced"
                },
